@model KKN_UKK.Models.User.Nasabah

@{
    ViewData["Title"] = "Detail Nasabah";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="card" style="width: auto;">
    <div class="card-body">
        <div>
            <h4>@ViewData["Title"]</h4>
            <hr />
            <dl class="row">
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.NIK)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.NIK)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.AccountNumber)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.AccountNumber)
                </dd>
                <dt class="col-sm-2">
                    Saldo
                </dt>
                <dd class="col-sm-10">
                    @ViewBag.Saldo
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Firstname)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Firstname)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.MidleName)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.MidleName)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.LastName)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.LastName)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Name)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Name)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Birtdate)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Birtdate)
                </dd>
                <dt class="col-sm-2">
                   Jenis Kelamin
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.JK)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Alamat)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Alamat)
                </dd>
            </dl>
        </div>
        <div>
            <a asp-action="EditNasabah" asp-route-id="@Model.id">Edit</a> |
            <a asp-action="ListNasabah">Back to List</a>
        </div>
    </div>
</div>


